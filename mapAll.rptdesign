            <xml-property name="queryText"><![CDATA[select 
t1.custId as custId,
t1.lng as lng, 
t1.lat as lat, 
t1.provinceName as provinceName, 
t1.cityName as cityName, 
t1.countyName as countyName, 
t1.address as address,
sum(t2.borrowAmount) as borrowAmount,
sum(t3.principal) as principal
from loan.custOpenAccPos t1
inner join `loan`.`loanBorrow` t2 on t1.custId = t2.custId and t2.status>=2
left join `loan`.`loanRepayment` t3 on t1.custId = t3.custId and t3.status>=2
where t1.custId not in(select id from `reports`.`cust_fake`)
group by t1.custId]]></xml-property>
